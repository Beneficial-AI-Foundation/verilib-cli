pub proof fn lemma_shr_51_le(a: u128, b: u128)
    requires
        a <= b
    ensures
        (a >> 51) <= (b >> 51)
{
    lemma_pow2_pos(51);
    lemma2_to64_rest(); // pow2(51) value
    lemma_u128_shr_is_div(a, 51);
    lemma_u128_shr_is_div(b, 51);
    lemma_div_is_ordered(a as int, b as int, 51);
}