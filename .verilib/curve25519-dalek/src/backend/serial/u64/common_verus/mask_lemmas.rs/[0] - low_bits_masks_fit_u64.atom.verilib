pub proof fn low_bits_masks_fit_u64(k: nat)
    requires
        k <= 64
    ensures
        low_bits_mask(k) <= u64::MAX
{
    lemma_low_bits_mask_values(); // lbm(0) = 0, lbm(64) = 2^64
    assert(low_bits_mask(64) <= u64::MAX) by (compute);
    if (k < 64){
        low_bits_mask_increases(k, 64);
    }
}