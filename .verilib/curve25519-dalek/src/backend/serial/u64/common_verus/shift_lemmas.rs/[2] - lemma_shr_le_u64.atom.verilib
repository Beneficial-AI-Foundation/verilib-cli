pub proof fn lemma_shr_le_u64(a: u64, b: u64, k: nat)
    requires
        a <= b,
        k < 64
    ensures
        (a >> k) <= (b >> k)
{
    lemma_pow2_pos(k);
    lemma_u64_shr_is_div(a, k as u64);
    lemma_u64_shr_is_div(b, k as u64);
    lemma_div_is_ordered(a as int, b as int, pow2(k) as int);
}